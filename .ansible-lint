---
# .ansible-lint

profile: production

exclude_paths:
  - .cache/
  - .github/
  - .gitignore
  - .ansible-lint
  - ./temporary-files/

skip_list:
  - "package-latest"

mock_modules:
  - community.general.shutdown
  - ansible.posix.authorized_key
  - community.docker.docker_container_exec
  - community.general.filesize
  - community.general.filesystem
  - ansible.posix.mount
